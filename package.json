{
  "name": "graphql-express",
  "version": "0.0.0-development",
  "description": "A simple express api using graphql",
  "main": "index.js",
  "author": "Ephraim Atta-Duncan",
  "license": "MIT",
  "dependencies": {
    "apollo-server-core": "^3.6.1",
    "apollo-server-express": "^3.6.1",
    "express": "^4.17.2",
    "graphql": "^16.2.0"
  },
  "scripts": {
    "watch": "nodemon --watch './**/*.{ts,graphql}' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "dev": "tsc-watch --onSuccess \"npm run watch\"",
    "start": "nodemon index.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
